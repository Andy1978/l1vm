// array demo
//
(main func)
	(set int64 1 zero 0)
	(set int64 1 one 1)
	(set int64 1 offset 8)
	(set double 1 x 23.0)
	(set double 1 y 42.0)
	(set double 1 a 0.0)
	(set double 1 b 0.0)
	(set double 2 z 0.0 0.0)
	// assign to array
	(x z [ zero ] =)
	(y z [ offset ] =)
	// get array variable
	(z [ zero ] a =)
	(z [ offset ] b =)
	(5 a 0 0 intr0)
	(7 0 0 0 intr0)
	(5 b 0 0 intr0)
	(7 0 0 0 intr0)
	(255 zero 0 0 intr0)
(funcend)
